  "id": "pack-artifact-hardcoded",
  "spec": {
    "model": {
      "name": "wine-artifact-hardcoded",
      "version": 1,
      "artifactNameTemplate": "my-training.zip"
    },
    "toolchain": "mlflow",
    "entrypoint": "main",
    "workDir": "packages/tests/e2e/robot/tests/local/resources/artifacts",
    "hyperParameters": {
      "alpha": "1.0"
    },
    "vcsName": "odahu-flow-examples"
  }
