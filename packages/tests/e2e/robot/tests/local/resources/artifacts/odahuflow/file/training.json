[
  {
    "kind": "TrainingIntegration",
    "id": "mlflow",
    "spec": {
      "defaultImage": "gcr.io/or2-msq-epmd-legn-t1iylu/odahu/odahu-flow-mlflow-training-integration:<tag>",
      "entrypoint": "/opt/conda/bin/odahu-flow-mlflow-runner"
    }
  },
  {
    "kind": "ModelTraining",
    "id": "local id file with spaces",
    "spec": {
      "entrypoint": "main",
      "hyperParameters": {
        "alpha": "1.0"
      },
      "model": {
        "artifactNameTemplate": "{{ .Name }}-{{ .Version }}.zip",
        "name": "wine-local",
        "version": 1
      },
      "trainingIntegration": "mlflow",
      "workDir": "packages/tests/e2e/robot/tests/local/resources/artifacts",
      "resources": {
        "limits": {
          "cpu": 1,
          "memory": "1Gi"
        },
        "requests": {
          "cpu": 1,
          "memory": "1Gi"
        }
      }
    }
  }
]
